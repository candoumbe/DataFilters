<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <Authors>Cyrillle NDOUMBE</Authors>
    <Product />
    <Description>Sets of classes to convert querystrings to strongly typed expressions.</Description>
    <PackageTags>expressions, querystring</PackageTags>
    <RepositoryType>git</RepositoryType>
    <Version>2.1.0</Version>
    <PackageReleaseNotes>Added support for `And` and `Or` operators to filter an element with multiple criterias
Added support for query string with `[` or `]` like : `Firstname=[Bb]ruce` which can be converted to `Firstname=Bruce|bruce`
</PackageReleaseNotes>
    <AssemblyVersion>$(Version)</AssemblyVersion>
    <FileVersion>$(Version)</FileVersion>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>github.com/candoumbe/DataFilters</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Primitives" Version="2.1.1" />
    <PackageReference Include="Roslynator.Analyzers" Version="1.9.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
    <PackageReference Include="System.Collections.Immutable" Version="1.5.0" />
    <PackageReference Include="Newtonsoft.Json.Schema" Version="3.0.10" />
    <PackageReference Include="Utilities" Version="1.1.0" />
  </ItemGroup>

</Project>
